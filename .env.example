# WEPL Lambda Function - Local Development Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# API URLs
HUG_API_URL=https://www.khug.or.kr/SelectListInfo.do?API_KEY=your_hug_api_key_here
LH_API_URL=https://apis.data.go.kr/1613000/HWSPR02/rsdtRcritNtcList?numOfRows=30&serviceKey=your_lh_api_key_here

# Database Configuration
DB_HOST=your_database_host_here
DB_PORT=3306
DB_NAME=your_database_name_here
DB_USER=your_database_user_here
DB_PASSWORD=your_database_password_here

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
KAKAO_API_KEY=your_kakao_api_key_here
YOUTUBE_API_KEY=your_youtube_api_key_here

# AWS Configuration (optional - will use AWS credentials from ~/.aws/ if not set)
AWS_REGION=ap-northeast-2
SECRET_NAME=wepl-lambda-secrets
