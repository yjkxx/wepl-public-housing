{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowECSAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::YOUR_ACCOUNT_ID:role/YOUR_STACK_NAME-ecs-task-role"
      },
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::wepl-mainpage",
        "arn:aws:s3:::wepl-mainpage/*"
      ]
    }
  ]
}
